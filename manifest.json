{
  "name": "Youtube Volume Control",
  "description": "Better and bigger custom youtube volume silder extension.",
  "version": "1.0.0",
  "manifest_version": 2,
  "homepage_url": "https://github.com/kotleni/yt-volume-slider-extension",
  "browser_action": {
  },
  "icons": {
    "48": "logo48.png"
  },
  "content_scripts" : [
    {
      "matches": ["*://*.youtube.com/watch*"],
      "js": ["index.js"],
      "css" : ["index.css"]
    }
  ]
}